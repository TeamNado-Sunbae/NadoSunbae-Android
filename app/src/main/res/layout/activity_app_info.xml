<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl_mypage_app_info"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:paddingBottom="12dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ImageView
                android:id="@+id/img_mypage_app_info_move_page"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="48dp"
                android:background="@drawable/btn_left_arrow"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/text_mypage_app_info"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/pretendard_medium"
                android:text="앱정보"
                android:textColor="@color/black"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="@+id/img_mypage_app_info_move_page"
                app:layout_constraintStart_toEndOf="@+id/img_mypage_app_info_move_page"
                app:layout_constraintTop_toTopOf="@+id/img_mypage_app_info_move_page" />


        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:id="@+id/text_mypage_app_info_rules_of_use"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginTop="21dp"
            android:fontFamily="@font/pretendard_regular"
            android:text="서비스 이용 규칙"
            android:textColor="@color/main_text"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/cl_mypage_app_info" />

        <ImageView
            android:id="@+id/text_mypage_app_info_rules_of_use_arrow"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="20dp"
            android:background="@drawable/btn_right_arrow"
            app:layout_constraintBottom_toBottomOf="@+id/text_mypage_app_info_rules_of_use"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/text_mypage_app_info_rules_of_use" />

        <View
            android:id="@+id/view_mypage_bar1"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="16dp"
            android:background="@color/gray_0"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/text_mypage_app_info_rules_of_use" />

        <TextView
            android:id="@+id/text_mypage_app_info_terms_of_use"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginTop="21dp"
            android:fontFamily="@font/pretendard_regular"
            android:text="서비스 이용약관"
            android:textColor="@color/main_text"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="@+id/text_mypage_app_info_rules_of_use"
            app:layout_constraintTop_toBottomOf="@+id/view_mypage_bar1" />

        <ImageView
            android:id="@+id/img_mypage_app_info_terms_of_use"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/btn_right_arrow"
            app:layout_constraintBottom_toBottomOf="@+id/text_mypage_app_info_terms_of_use"
            app:layout_constraintEnd_toEndOf="@+id/text_mypage_app_info_rules_of_use_arrow"
            app:layout_constraintStart_toStartOf="@+id/text_mypage_app_info_rules_of_use_arrow"
            app:layout_constraintTop_toBottomOf="@+id/text_mypage_app_info_terms_of_use" />

        <View
            android:id="@+id/view_mypage_bar2"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="21dp"
            android:layout_marginEnd="16dp"
            android:background="@color/gray_0"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/text_mypage_app_info_terms_of_use" />

        <TextView
            android:id="@+id/text_mypage_app_info_opensource"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginTop="21dp"
            android:fontFamily="@font/pretendard_regular"
            android:text="오픈소스 라이선스"
            android:textColor="@color/main_text"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="@+id/text_mypage_setting_chagne_pw"
            app:layout_constraintTop_toBottomOf="@+id/view_mypage_bar2" />

        <ImageView
            android:id="@+id/img_mypage_app_info_opensource"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/btn_right_arrow"
            app:layout_constraintBottom_toBottomOf="@+id/text_mypage_app_info_opensource"
            app:layout_constraintEnd_toEndOf="@+id/img_mypage_app_info_terms_of_use"
            app:layout_constraintStart_toStartOf="@+id/img_mypage_app_info_terms_of_use"
            app:layout_constraintTop_toTopOf="@+id/text_mypage_app_info_opensource" />


        <View
            android:id="@+id/view_mypage_bar3"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="16dp"
            android:background="@color/gray_0"
            app:layout_constraintEnd_toEndOf="@+id/text_mypage_app_info_opensource"
            app:layout_constraintStart_toStartOf="@+id/text_mypage_app_info_opensource"
            app:layout_constraintTop_toBottomOf="@+id/text_mypage_app_info_opensource" />

        <TextView
            android:id="@+id/text_mypage_app_info_version_info"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginTop="21dp"
            android:fontFamily="@font/pretendard_regular"
            android:text="버전정보"
            android:textColor="@color/main_text"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="@+id/text_mypage_app_info_opensource"
            app:layout_constraintTop_toBottomOf="@+id/view_mypage_bar3" />

        <TextView
            android:id="@+id/text_mypage_app_info_recent_version"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="24dp"
            android:fontFamily="@font/pretendard_regular"
            android:textColor="@color/gray_4"
            android:textSize="14sp"
            android:layout_marginTop="21dp"
            app:layout_constraintBottom_toBottomOf="@+id/text_mypage_setting_service_inquery"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view_mypage_bar3"
            tools:text="1.0.0" />

        <TextView
            android:id="@+id/text_mypage_app_info_recent_version_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/pretendard_regular"
            android:text=" / 최신 "
            android:textColor="@color/gray_4"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="@+id/text_mypage_app_info_recent_version"
            app:layout_constraintEnd_toStartOf="@+id/text_mypage_app_info_recent_version"
            app:layout_constraintTop_toTopOf="@+id/text_mypage_app_info_recent_version" />


        <TextView
            android:id="@+id/text_mypage_app_info_now_version"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="21dp"
            android:fontFamily="@font/pretendard_regular"
            android:textColor="@color/gray_4"
            android:textSize="14sp"
            app:layout_constraintEnd_toStartOf="@+id/text_mypage_app_info_recent_version_title"
            app:layout_constraintTop_toBottomOf="@+id/view_mypage_bar3"
            tools:text="1.0.0" />

        <TextView
            android:id="@+id/text_mypage_app_info_now_version_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/pretendard_regular"
            android:text="현재 "
            android:textColor="@color/gray_4"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="@+id/text_mypage_app_info_recent_version"
            app:layout_constraintEnd_toStartOf="@+id/text_mypage_app_info_now_version"
            app:layout_constraintTop_toTopOf="@+id/text_mypage_app_info_recent_version" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>