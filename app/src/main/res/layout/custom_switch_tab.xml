<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/rectangle_gray5_8"
        android:paddingEnd="4dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/switch_tab_one"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginVertical="4dp"
            android:layout_marginStart="4dp"
            android:background="@drawable/selector_renctangle_gray5_to_white"
            android:fontFamily="@font/pretendard_medium"
            android:lineHeight="22sp"
            android:paddingHorizontal="14dp"
            android:paddingVertical="3dp"
            android:text="@string/navigation_room"
            android:textColor="@drawable/selector_gray_3_to_main_default"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/switch_tab_two"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:background="@drawable/selector_renctangle_gray5_to_white"
            android:fontFamily="@font/pretendard_medium"
            android:lineHeight="22sp"
            android:paddingHorizontal="14dp"
            android:paddingVertical="3dp"
            android:text="@string/navigation_room"
            android:textColor="@drawable/selector_gray_3_to_main_default"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="@+id/switch_tab_one"
            app:layout_constraintStart_toEndOf="@+id/switch_tab_one"
            app:layout_constraintTop_toTopOf="@+id/switch_tab_one" />

        <TextView
            android:id="@+id/switch_tab_three"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginVertical="4dp"
            android:layout_marginStart="8dp"
            android:background="@drawable/selector_renctangle_gray5_to_white"
            android:fontFamily="@font/pretendard_medium"
            android:lineHeight="22sp"
            android:paddingHorizontal="14dp"
            android:paddingVertical="3dp"
            android:text="@string/navigation_room"
            android:textColor="@drawable/selector_gray_3_to_main_default"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="@+id/switch_tab_two"
            app:layout_constraintStart_toEndOf="@+id/switch_tab_two"
            app:layout_constraintTop_toTopOf="@+id/switch_tab_two" />

        <TextView
            android:id="@+id/switch_tab_four"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginVertical="4dp"
            android:layout_marginStart="8dp"
            android:background="@drawable/selector_renctangle_gray5_to_white"
            android:fontFamily="@font/pretendard_medium"
            android:lineHeight="22sp"
            android:paddingHorizontal="14dp"
            android:paddingVertical="3dp"
            android:text="@string/navigation_room"
            android:textColor="@drawable/selector_gray_3_to_main_default"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="@+id/switch_tab_three"
            app:layout_constraintStart_toEndOf="@+id/switch_tab_three"
            app:layout_constraintTop_toTopOf="@+id/switch_tab_three" />


    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>