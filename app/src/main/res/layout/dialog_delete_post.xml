<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:paddingBottom="14dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/tv_dialog_delete_title"
        android:layout_width="230dp"
        android:layout_height="88dp"
        android:layout_marginTop="12dp"
        android:layout_marginHorizontal="24dp"
        android:lineHeight="22sp"
        android:letterSpacing="-0.009"
        android:fontFamily="@font/pretendard_regular"
        android:text="@string/notification_delete_post"
        android:textAlignment="center"
        android:textColor="@color/main_text"
        android:textSize="14sp"
        android:gravity="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />




    <TextView
        android:id="@+id/btn_dialog_complete"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@drawable/rectangle_fill_black_12dp"
        android:fontFamily="@font/pretendard_regular"
        android:paddingLeft="32dp"
        android:paddingTop="15dp"
        android:paddingRight="32dp"
        android:paddingBottom="15dp"
        android:text="@string/findpw_check"
        android:textColor="@color/main_default"
        android:textSize="14sp"
        android:gravity="center"
        android:layout_marginStart="22dp"
        android:layout_marginEnd="22dp"
       app:layout_constraintTop_toBottomOf="@+id/tv_dialog_delete_title"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>