<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@drawable/rectangle_fill_white_top_8dp">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_filter_bottom_sheet"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/tv_bottomsheeet_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="24dp"
            android:fontFamily="@font/pretendard_medium"
            android:text="@string/mypage_modify_img"
            android:textColor="@color/black"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageButton
            android:id="@+id/btn_bottomsheet_cancel"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginTop="12dp"
            android:layout_marginEnd="16dp"
            android:background="@drawable/btn_x_black"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/view_bottomsheet_main_line"
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="15dp"
            android:layout_marginEnd="16dp"
            android:background="@color/main_default"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_bottomsheeet_title" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="40dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view_bottomsheet_main_line">


            <ImageView
                android:id="@+id/img_bottomsheet_1"
                android:layout_width="72dp"
                android:layout_height="72dp"
                android:src="@drawable/c_72"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/img_bottomsheet_2"
                android:layout_width="72dp"
                android:layout_height="72dp"
                android:layout_marginStart="16dp"
                android:src="@drawable/e_72"
                app:layout_constraintBottom_toBottomOf="@+id/img_bottomsheet_1"
                app:layout_constraintStart_toEndOf="@+id/img_bottomsheet_1"
                app:layout_constraintTop_toTopOf="@+id/img_bottomsheet_1" />

            <ImageView
                android:id="@+id/img_bottomsheet_3"
                android:layout_width="72dp"
                android:layout_height="72dp"
                android:layout_marginStart="16dp"
                android:src="@drawable/b_72"
                app:layout_constraintStart_toEndOf="@+id/img_bottomsheet_2"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/iv_checked_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/group_878"
                android:visibility="invisible"
                app:layout_constraintEnd_toEndOf="@+id/img_bottomsheet_1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/img_bottomsheet_1" />

            <ImageView
                android:id="@+id/iv_checked_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/group_878"
                android:visibility="invisible"
                app:layout_constraintStart_toStartOf="@+id/img_bottomsheet_2"
                app:layout_constraintTop_toTopOf="@+id/img_bottomsheet_2" />


            <ImageView
                android:id="@+id/iv_checked_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/group_878"
                android:visibility="invisible"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@+id/img_bottomsheet_3"
                app:layout_constraintTop_toTopOf="@+id/img_bottomsheet_3" />

        </androidx.constraintlayout.widget.ConstraintLayout>


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="24dp"
            app:layout_constraintEnd_toEndOf="@+id/constraintLayout5"
            app:layout_constraintStart_toStartOf="@+id/constraintLayout5"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout5">

            <ImageView
                android:id="@+id/img_bottomsheet_4"
                android:layout_width="72dp"
                android:layout_height="72dp"
                android:src="@drawable/d_72"
                app:layout_constraintBottom_toBottomOf="@+id/img_bottomsheet_5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />


            <ImageView
                android:id="@+id/img_bottomsheet_5"
                android:layout_width="72dp"
                android:layout_height="72dp"
                android:layout_marginStart="16dp"
                android:src="@drawable/a_72"
                app:layout_constraintStart_toEndOf="@+id/img_bottomsheet_4"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/iv_checked_4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/group_878"
                android:visibility="invisible"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/img_bottomsheet_4" />

            <ImageView
                android:id="@+id/iv_checked_5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/group_878"
                android:visibility="invisible"
                app:layout_constraintStart_toStartOf="@+id/img_bottomsheet_5"
                app:layout_constraintTop_toTopOf="@+id/img_bottomsheet_4" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <android.widget.Button
            android:id="@+id/btn_filter_apply"
            style="?android:attr/borderlessButtonStyle"
            android:layout_width="0dp"
            android:layout_height="60dp"
            android:layout_marginStart="32dp"
            android:layout_marginTop="56dp"
            android:layout_marginEnd="32dp"
            android:layout_marginBottom="20dp"
            android:background="@drawable/selector_rectangle_enabled_gray_to_black_14dp"
            android:enabled="true"
            android:fontFamily="@font/pretendard_semibold"
            android:paddingVertical="19dp"
            android:text="@string/bottom_sheet_complete"
            android:textAlignment="center"
            android:textColor="@drawable/selector_gray_3_to_main_default_enabled"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout7"
            app:layout_constraintVertical_bias="0.0" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>